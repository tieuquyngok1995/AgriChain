{
  "name": "agri-chain-server",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon",
    "test": "node test-blockchain.js && node test-api.js && node test-agrichain.js",
    "test:blockchain": "node test-blockchain.js",
    "test:api": "node test-api.js",
    "test:agrichain": "node test-agrichain.js",
    "test:files": "node test-file-upload.js",
    "test:file-services": "node test-file-services.js",
    "test:comprehensive": "node test-comprehensive.js",
    "test:database-integration": "node test-database-integration.js",
    "test:all": "node test-blockchain.js && node test-api.js && node test-agrichain.js && node test-file-upload.js",
    "generate-wallet": "node generate-wallet.js",
    "db:setup": "echo 'Run: sqlcmd -S your_server -i database/create_simple_tables.sql'",
    "db:setup-full": "echo 'Run: sqlcmd -S your_server -i database/create_agrichain_database_sqlserver.sql'",
    "db:procedures": "echo 'Run: sqlcmd -S your_server -i database/stored_procedures.sql'"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "express": "^4.19.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "mssql": "^12.0.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "winston": "^3.18.3"
  }
}